IDBox.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
IDBox.BorderColor3 = Color3.fromRGB(0, 0, 0)
IDBox.Position = UDim2.new(0.153578788, 0, 0.406474829, 0)
IDBox.Size = UDim2.new(0, 353, 0, 40)
IDBox.Font = Enum.Font.SourceSans
IDBox.PlaceholderText = "User ID"
IDBox.Text = ""
IDBox.TextColor3 = Color3.fromRGB(255, 255, 255)
IDBox.TextSize = 14.000

Confirm.Name = "Confirm"
Confirm.Parent = MainFrame
Confirm.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Confirm.BorderSizePixel = 0
Confirm.Position = UDim2.new(0.293577999, 0, 0.633093536, 0)
Confirm.Size = UDim2.new(0, 200, 0, 44)
Confirm.Font = Enum.Font.SourceSans
Confirm.Text = "Confirm"
Confirm.TextColor3 = Color3.fromRGB(255, 255, 255)
Confirm.TextSize = 14.000
Confirm.MouseButton1Click:Connect(function()
	if IDBox.Text == "2385124288" then
		if game.Players.LocalPlayer.Name == "Pockie_Potato" or game.Players.LocalPlayer.Name == "Switchtone2" then
			SecruityTop.Visible = false
			MainTop.Visible = true
		elseif IDBox.Text == "2852432638" then
			SecruityTop.Visible = false
			MainTop.Visible = true
		elseif IDBox.Text == "1249356356" then
			SecruityTop.Visible = false
			MainTop.Visible = true	
		end
	end
end)


Player.Chatted:Connect(function(msg)
	if msg == "/remove" then
		PotatoHubV5:Destroy()
	end
end)

UIS.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.RightAlt then
		if MainTop.Visible == true then
			MainTop.Visible = false
		else
			MainTop.Visible = true
		end
	end
end)
