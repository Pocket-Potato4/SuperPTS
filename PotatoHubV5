HideFistAura.Name = "HideFistAura"
HideFistAura.Parent = FunctionTitle_5
HideFistAura.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
HideFistAura.BorderSizePixel = 0
HideFistAura.Position = UDim2.new(2.69148946, 0, 0.214664564, 0)
HideFistAura.Size = UDim2.new(0, 44, 0, 37)
HideFistAura.Font = Enum.Font.SourceSans
HideFistAura.Text = ""
HideFistAura.TextColor3 = Color3.fromRGB(0, 0, 0)
HideFistAura.TextSize = 14.000
HideFistAura.MouseButton1Click:Connect(function()
	if HideFistAura.BackgroundColor3 == Color3.fromRGB(30, 30, 30) then
		HideFistAura.BackgroundColor3 = Color3.fromRGB(13, 202, 0)
		repeat 
			wait()	
			local aura1 = game.Players.LocalPlayer.Character.LeftHand:FindFirstChild("Aura1")
			local aura2 = game.Players.LocalPlayer.Character.LeftHand:FindFirstChild("Aura2")
			local aura3 = game.Players.LocalPlayer.Character.RightHand:FindFirstChild("Aura1")
			local aura4 = game.Players.LocalPlayer.Character.RightHand:FindFirstChild("Aura2")
			
			if aura1 then
				aura1:Destroy()
			end
			
			if aura2 then
				aura2:Destroy()
			end
			
			if aura3 then
				aura3:Destroy()
			end
			
			if aura4 then
				aura4:Destroy()
			end
		until HideFistAura.BackgroundColor3 == Color3.fromRGB(30, 30, 30)
	else	
		HideFistAura.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	end
end)

UICorner_5.Parent = HideFistAura

