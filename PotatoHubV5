local maxhp = game.Players.LocalPlayer.Character.Humanoid.Health

function respawn()
	local p0 = game.Players.LocalPlayer.Character.UpperTorso.Position.X
	local p1 = game.Players.LocalPlayer.Character.UpperTorso.Position.Y
	local p2 = game.Players.LocalPlayer.Character.UpperTorso.Position.Z

	Player.Character:MoveTo(Vector3.new(0,15000,0))
	Event:FireServer({"Respawn"})
	game.Players.LocalPlayer.Character.Humanoid:Remove()
	wait(2)
	Player.Character:MoveTo(vector3.new(p0,p1,p2))
	
end



