AutoRespawn.Name = "AutoRespawn"
AutoRespawn.Parent = FunctionTitle_3
AutoRespawn.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
AutoRespawn.BorderSizePixel = 0
AutoRespawn.Position = UDim2.new(2.69148946, 0, 0.214664564, 0)
AutoRespawn.Size = UDim2.new(0, 44, 0, 37)
AutoRespawn.Font = Enum.Font.SourceSans
AutoRespawn.Text = ""
AutoRespawn.TextColor3 = Color3.fromRGB(0, 0, 0)
AutoRespawn.TextSize = 14.000
AutoRespawn.MouseButton1Click:Connect(function()
	if AutoRespawn.BackgroundColor3 == Color3.fromRGB(30, 30, 30)  then
		AutoRespawn.BackgroundColor3 = Color3.fromRGB(13, 202, 0)
		
		repeat
			wait()
			if game.Players.LocalPlayer.Character.Humanoid.Health ~= maxhp then
				respawn()
			end
		until AutoRespawn.BackgroundColor3 == Color3.fromRGB(30, 30, 30)
	else
		AutoRespawn.BackgroundColor3 = Color3.fromRGB(30,30,30)
	end
end)

