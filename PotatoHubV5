TextButton.Parent = FunctionTitle_6
TextButton.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
TextButton.BorderSizePixel = 0
TextButton.Position = UDim2.new(2.69148946, 0, 0.214664564, 0)
TextButton.Size = UDim2.new(0, 44, 0, 37)
TextButton.Font = Enum.Font.SourceSans
TextButton.Text = ""
TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
TextButton.TextSize = 14.000
TextButton.MouseButton1Click:Connect(function()
	if TextButton.BackgroundColor3 == Color3.fromRGB(30, 30, 30) then
		TextButton.BackgroundColor3 = Color3.fromRGB(13, 202, 0)
		repeat 
			wait()	
			local aura1 = game.Players.LocalPlayer.Character.UpperTorso:FindFirstChild("Aura1")
			local aura2 = game.Players.LocalPlayer.Character.RightUpperLeg:FindFirstChild("Aura1")
			local aura3 = game.Players.LocalPlayer.Character.LeftUpperLeg:FindFirstChild("Aura1")

			if aura1 then
				aura1:Destroy()
			end

			if aura2 then
				aura2:Destroy()
			end

			if aura3 then
				aura3:Destroy()
			end

			
		until TextButton.BackgroundColor3 == Color3.fromRGB(30, 30, 30)
	else	
		TextButton.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	end
end)

