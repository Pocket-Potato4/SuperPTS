Teleport.Text = "TP"
Teleport.TextColor3 = Color3.fromRGB(255, 255, 255)
Teleport.TextSize = 14.000
Teleport.MouseButton1Click:Connect(function()
	if DropDown.Text == "Spawn" then
		Player.Character:MoveTo(Vector3.new(430, 249, 878))
	elseif DropDown.Text == "CityPort" then
		Player.Character:MoveTo(Vector3.new(335, 263, -409))
	elseif DropDown.Text == "IceBerg" then	
		Player.Character:MoveTo(Vector3.new(1565, 254, 2227))
	elseif DropDown.Text == "Tornado" then
		Player.Character:MoveTo(Vector3.new(-2290, 969, 1082))
	elseif DropDown.Text == "Volcano" then
		Player.Character:MoveTo(Vector3.new(-2047, 731, -1830))
	elseif DropDown.Text == "Crystal" then
		Player.Character:MoveTo(Vector3.new(-2254, 1943, 1046))
	elseif DropDown.Text == "Blue Star" then
		Player.Character:MoveTo(Vector3.new(1176, 4789, -2293))
	elseif DropDown.Text == "Green Star" then
		Player.Character:MoveTo(Vector3.new(1380, 9274, 1648))
	elseif DropDown.Text == "Orange Star" then
		Player.Character:MoveTo(Vector3.new(-372, 15734, -12))
	elseif DropDown.Text == "Zen Island" then
		Player.Character:MoveTo(Vector3.new(-2601, 5533, -504))
	elseif DropDown.Text == "Grim Reapers Basement" then	
		Player.Character:MoveTo(Vector3.new(-246, 290, 958))
	end
end)

