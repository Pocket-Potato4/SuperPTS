Plr.Chatted:Connect(function(msg)
	if msg == "/e destroy" then
		ScreenGui:Destroy()
		Top:Destroy()
	end
end)

UIP.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.Minus then
		if Top.Visible == true then
			Top.Visible = false
		else
			Top.Visible = true
		end
	end	
end)
